global proc selectRightFingers(){
if (`objExists R_thumb_01_ctrl`)
{ 
select -r R_thumb_01_ctrl;
}

if (`objExists R_index_01_ctrl`)
{ 
select -add R_index_01_ctrl;
}

if (`objExists R_middle_01_ctrl`)
{ 
select -add R_middle_01_ctrl;
}

if (`objExists R_ring_01_ctrl`)
{ 
select -add R_ring_01_ctrl;
}

if (`objExists R_pinky_01_ctrl`)
{ 
select -add R_pinky_01_ctrl;
}
}